<Window x:Class="GUI.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Window3" Height="450" Width="800" Background="#545d6a">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button x:Name="nowyButton" Background="White" Opacity="0.9" Padding="0, 4" Height="34" Content="Stwórz nowy projekt&#xD;&#xA;" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" Width="296" Margin="240,100,0,0" />
        <Button x:Name="aktualneButton" Background="White" Opacity="0.9" Padding="0, 4" Height="34" Content="Przeglądaj aktualne projekty&#xA;" FontSize="16" Margin="240,180,0,205" HorizontalAlignment="Left" Width="296"/>
        <Button x:Name="archiwumButton" Background="White" Opacity="0.9" Padding="0, 4" Height="34" Content="Otwórz archiwum projektów&#xA;" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" Width="296" Margin="240,260,0,0" />

    </Grid>
</Window>
